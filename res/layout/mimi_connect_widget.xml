<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    android:layout_margin="@dimen/widget_margin"
    android:background="@color/mimiTransparentColor"
    android:baselineAligned="false"
    android:orientation="vertical"
    android:padding="@dimen/widget_margin" >
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@null"
        android:orientation="horizontal" >
        <LinearLayout
            android:id="@+id/widgetmsora"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:layout_weight="0.1"
            android:background="@null"
            android:orientation="vertical" >
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:padding="5dp"
                android:src="@drawable/mc_main_logo" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/widgetSearchPhonebook"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="5dp"
            android:layout_weight="0.3"
            android:background="@null"
            android:orientation="vertical" >
            <TextView
                android:id="@+id/widgetTxtSearchBox"
                style="@android:style/Widget.EditText"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:background="@drawable/mimi_profile_edittext"
                android:drawableRight="@drawable/mimi_connect_search"
                android:hint="@string/hintSearchPersonalContacts"
                android:padding="5dp" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/widgetPhonebook"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="0.2"
            android:background="@null"
            android:orientation="vertical" >
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:padding="5dp"
                android:src="@drawable/mimi_connect_personal_phonebook" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/widgetLivelink"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="0.2"
            android:background="@null"
            android:orientation="vertical" >
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:padding="5dp"
                android:src="@drawable/mimi_connect_livelink" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/widgetNewContact"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="0.2"
            android:background="@null"
            android:orientation="vertical" >
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:padding="5dp"
                android:src="@drawable/mimi_connect_contactphoto" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/widgetFirstDialPanel"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/mimi_connect_transparent_mimi_background"
        android:orientation="vertical" >
        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="5dp"
            android:background="@color/mimiTransparentClearColor"
            android:gravity="center"
            android:text="Fast Dial"
            android:textColor="@color/black"
            android:textSize="15dp"
            android:textStyle="bold" />
        <LinearLayout
            android:id="@+id/widgetFirstDial0"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/mimi_connect_transparent_mimi_background"
            android:orientation="horizontal" >
            <TextView
                android:id="@+id/widgetTxtAddView0"
                style="@android:style/Widget.EditText"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:layout_weight="0.7"
                android:autoLink="web"
                android:background="@null"
                android:gravity="center_vertical"
                android:linksClickable="true"
                android:textColor="@color/white"
                android:textSize="12dp"
                android:textStyle="bold|italic" />
            <LinearLayout
                android:id="@+id/widgetCall0"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:layout_marginRight="5dp"
                android:layout_weight="0.15"
                android:background="@color/mimiTransparentColor"
                android:orientation="vertical" >
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:padding="5dp"
                    android:src="@drawable/mimi_connect_call" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/widgetText0"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:layout_weight="0.15"
                android:background="@color/mimiTransparentColor"
                android:orientation="vertical" >
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:padding="5dp"
                    android:src="@drawable/mimi_connect_sendtext" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/widgetFirstDial1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/mimi_connect_transparent_mimi_background"
            android:orientation="horizontal" >
            <TextView
                android:id="@+id/widgetTxtAddView1"
                style="@android:style/Widget.EditText"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:layout_weight="0.7"
                android:autoLink="web"
                android:background="@null"
                android:gravity="center_vertical"
                android:linksClickable="true"
                android:textColor="@color/white"
                android:textSize="12dp"
                android:textStyle="bold|italic" />
            <LinearLayout
                android:id="@+id/widgetCall1"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:layout_marginRight="5dp"
                android:layout_weight="0.15"
                android:background="@color/mimiTransparentColor"
                android:orientation="vertical" >
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:padding="5dp"
                    android:src="@drawable/mimi_connect_call" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/widgetText1"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:layout_weight="0.15"
                android:background="@color/mimiTransparentColor"
                android:orientation="vertical" >
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:padding="5dp"
                    android:src="@drawable/mimi_connect_sendtext" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/widgetFirstDial2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/mimi_connect_transparent_mimi_background"
            android:orientation="horizontal" >
            <TextView
                android:id="@+id/widgetTxtAddView2"
                style="@android:style/Widget.EditText"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:layout_weight="0.7"
                android:autoLink="web"
                android:background="@null"
                android:gravity="center_vertical"
                android:linksClickable="true"
                android:textColor="@color/white"
                android:textSize="12dp"
                android:textStyle="bold|italic" />
            <LinearLayout
                android:id="@+id/widgetCall2"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:layout_marginRight="5dp"
                android:layout_weight="0.15"
                android:background="@color/mimiTransparentColor"
                android:orientation="vertical" >
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:padding="5dp"
                    android:src="@drawable/mimi_connect_call" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/widgetText2"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:layout_weight="0.15"
                android:background="@color/mimiTransparentColor"
                android:orientation="vertical" >
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:padding="5dp"
                    android:src="@drawable/mimi_connect_sendtext" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>